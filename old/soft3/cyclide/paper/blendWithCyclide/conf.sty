\long\def\@makecaption#1#2{
 \vskip 1ex
 \setbox\@tempboxa\hbox{ {\sc #1.} \strut\ #2}
 \ifdim \wd\@tempboxa >\hsize
   \unhbox\@tempboxa\protect\centering\par
 \else
   \hbox to \hsize{\hfil\box\@tempboxa\hfil}
\fi}

